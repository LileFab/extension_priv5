{
    "manifest_version": 3,
    "name": "Statut priv 5",
    "description": "Vérifier la disponibilité de priv 5",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "storage",
        "notifications",
        "https://api.fleisch.fr/*"
    ],
    "host_permissions": [
        "https://api.fleisch.fr/*"
    ],
    "background": {
        "service_worker": "background/service-worker.js"
    }
}